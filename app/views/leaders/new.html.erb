<div id="center_it">
<h1>Nouveau Leader</h1>
<%= semantic_form_for(@leader) do |f| %>
    <% f.inputs do %>
        <%= f.input :first_name %>
        <%= f.input :last_name %>
        <%= f.input :email %>
        <%= f.input :role %>
        <%= f.input :agence %>
    <% end %>

      <% f.buttons do %>
          <%= f.commit_button %>
      <% end %>
<% end %>
</div>