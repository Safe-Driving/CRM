<div id="center_it">
<h1>Editer la Formation</h1>

<%= semantic_form_for(@formation) do |f| %>
      <% f.inputs do %>
          <%= f.input :date %>
          <%= f.input :agency %>
          <%= f.input :type %>
          <%= f.input :users_number %>
          <%= f.input :trainer %>
          <%= f.input :place %>
      <% end %>

      <% f.buttons do %>
          <%= f.commit_button %>
      <% end %>
<% end %>
</div>