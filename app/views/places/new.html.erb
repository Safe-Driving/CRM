<div id="center_it">
<h1>Nouveau lieu</h1>
<%= semantic_form_for(@place) do |f| %>
      <% f.inputs do %>
          <%= f.input :name %>
          <%= f.input :city %>
          <%= f.input :address %>

      <% end %>

      <% f.buttons do %>
          <%= f.commit_button %>
      <% end %>
<% end %>
</div>